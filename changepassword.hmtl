<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <script src="js/jquery-3.2.1.min.js"></script>
    <title>Ikinci El Satis</title>
    <script>
        $(document).ready(function () {
            $("#forgetpassword_submit").click(function () {
				var name = $("#getpass_name").val();
				var lastname=$("#getpass_lastname").val();
				var	username=$("#getpass_username").val();
                var phone=$("#getpass_phone").val();
                var birthday=$("#getpass_birthday").val();
				var email=$("#register_email").val();
                var city=$("#getpass_cities").val();
                var countity=$("#getpass_countity").val();
				var data = JSON.stringify({ "name": name,"lastname":lastname,"phone":phone,"birthday":birthday,"email":email,"city":city,"countity":countity,"username":username});
                $.ajax({
                    type: "POST",
                    dataType: "json",
                    url: "api/"+/* burası değiştirilecek  */+".php",
                    data: data,
                    success: function (data) {
                        window.location
                    }
                });
                return false; 
            });
        });

    </script>
</head>
<body>
<div id="indexpanel">
	<label id="getpass_namelabel" for="getpass_name">İsim:</label><input type="text" class="getpasstext" id="getpass_name" placeholder="" /><br/>
	<label id="getpass_lastnamelabel" for="getpass_lastname">Soyisim:</label><input type="text" class="getpasstext" id="getpass_lastname" placeholder="" /><br/>
	<label id="getpass_usernamelabel" for="getpass_username">Kullanıcı Adı:</label><input type="text" class="getpasstext" id="getpass_username" placeholder="" /><br/>
	<label id="getpass_phonelabel" for="getpass_phone">Telefon:</label><input type="text" class="getpasstext" id="getpass_phone" placeholder="" /><br/>
	<label id="getpass_birthdaylabel" for="getpass_birthday">Doğum Tarihi:</label><input type="text" class="getpasstext" id="getpass_birthday" placeholder="" /><br/>
	<label id="register_emaillabel" for="register_email">Email:</label><input type="text" class="registertext" id="register_email" placeholder="" /><br/>
	<label id="getpass_citieslabel" for="getpass_cities">İl:</label>
	<select id="getpass_cities">
		<option class="getpass_hiddenoption" value="" hidden><span class="getpass_hiddenoption_text"></span></option>
	</select>
	<br/>
	<label id="getpass_countitylabel" for="getpass_countity">İlçe:</label>
	<select id="getpass_countity">
		<option class="getpass_hiddenoption" value="" hidden><span class="getpass_hiddenoption_text"></span></option>
	</select>
</div>        
    <div id="footer">
	</div>
</body>
</html>